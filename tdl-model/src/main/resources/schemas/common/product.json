{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "productCode",
    "productName",
    "productDescription",
    "sellerName",
    "productPrice",
    "qty",
    "tags",
    "status",
    "img"
  ],
  "properties": {
    "productCode": {
      "$id": "#/properties/productCode",
      "type": "integer",
      "title": "The Productcode Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "productName": {
      "$id": "#/properties/productName",
      "type": "string",
      "title": "The Productname Schema",
      "default": "",
      "examples": [
        "Super Amazing Awesome Cake"
      ],
      "pattern": "^(.*)$"
    },
    "productDescription": {
      "$id": "#/properties/productDescription",
      "type": "string",
      "title": "The Productdescription Schema",
      "default": "",
      "examples": [
        "This cake is super duper amazing"
      ],
      "pattern": "^(.*)$"
    },
    "sellerName": {
      "$id": "#/properties/sellerName",
      "type": "string",
      "title": "The Sellername Schema",
      "default": "",
      "examples": [
        "Gordon Ramsay"
      ],
      "pattern": "^(.*)$"
    },
    "productPrice": {
      "$id": "#/properties/productPrice",
      "type": "integer",
      "title": "The Productprice Schema",
      "default": 0,
      "examples": [
        12350
      ]
    },
    "qty": {
      "$id": "#/properties/qty",
      "type": "integer",
      "title": "The Qty Schema",
      "default": 0,
      "examples": [
        25
      ]
    },
    "tags": {
      "$id": "#/properties/tags",
      "type": "array",
      "title": "The Tags Schema",
      "items": {
        "$id": "#/properties/tags/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "cake",
          "sugar"
        ],
        "pattern": "^(.*)$"
      }
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "A"
      ],
      "pattern": "^(.*)$"
    },
    "img": {
      "$id": "#/properties/img",
      "type": "string",
      "title": "The Img Schema",
      "default": "",
      "examples": [
        "https://images.immediate.co.uk/production/volatile/sites/2/2019/04/Choc-Fudge-Cake-b2d1909.jpg?quality=45&resize=620,413"
      ],
      "pattern": "^(.*)$"
    }
  }
}
