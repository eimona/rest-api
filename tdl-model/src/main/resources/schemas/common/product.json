{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "productCode",
    "productName",
    "productDescription",
    "sellerName",
    "productPrice",
    "qty",
    "tag",
    "status",
    "img"
  ],
  "properties": {
    "productCode": {
      "$id": "#/properties/productCode",
      "type": "integer",
      "title": "The Productcode Schema",
      "default": 0,
      "examples": [
        2
      ]
    },
    "productName": {
      "$id": "#/properties/productName",
      "type": "string",
      "title": "The Productname Schema",
      "default": "",
      "examples": [
        "Not So Amazing Cake"
      ],
      "pattern": "^(.*)$"
    },
    "productDescription": {
      "$id": "#/properties/productDescription",
      "type": "string",
      "title": "The Productdescription Schema",
      "default": "",
      "examples": [
        "This cake is not so amazing"
      ],
      "pattern": "^(.*)$"
    },
    "sellerName": {
      "$id": "#/properties/sellerName",
      "type": "string",
      "title": "The Sellername Schema",
      "default": "",
      "examples": [
        "Gordon Ramsay"
      ],
      "pattern": "^(.*)$"
    },
    "productPrice": {
      "$id": "#/properties/productPrice",
      "type": "integer",
      "title": "The Productprice Schema",
      "default": 0,
      "examples": [
        1234
      ]
    },
    "qty": {
      "$id": "#/properties/qty",
      "type": "integer",
      "title": "The Qty Schema",
      "default": 0,
      "examples": [
        25
      ]
    },
    "tag": {
      "$id": "#/properties/tag",
      "type": "string",
      "title": "The Tag Schema",
      "default": "",
      "examples": [
        "cake"
      ],
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "U"
      ],
      "pattern": "^(.*)$"
    },
    "img": {
      "$id": "#/properties/img",
      "type": "string",
      "title": "The Img Schema",
      "default": "",
      "examples": [
        "https://7yearolds.com/wp-content/uploads/2018/11/cake.jpgZ"
      ],
      "pattern": "^(.*)$"
    }
  }
}
